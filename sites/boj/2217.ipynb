    "n = int(input())\n",
    "\n",
    "arr = []\n",
    "for _ in range(n):\n",
    "    arr.append(int(input()))\n",
    "    \n",
    "arr.sort(reverse=True)\n",
    "maxVal = 0\n",
    "for i in range(n):\n",
    "    maxVal = max(maxVal, arr[i] * (i + 1))\n",
    "    \n",
    "print(maxVal)\n"
