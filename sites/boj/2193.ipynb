   "source": [
    "n = int(input())\n",
    "\n",
    "dp = [[0] * 2 for _ in range(n + 1)]\n",
    "dp[1][0] = 0\n",
    "dp[1][1] = 1\n",
    "\n",
    "for i in range(2, n + 1):\n",
    "    dp[i][0] = dp[i-1][0] + dp[i-1][1]\n",
    "    dp[i][1] = dp[i-1][0]\n",
    "    \n",
    "print(dp[n][0] + dp[n][1])"
   ]
