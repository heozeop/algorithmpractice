    "\n",
    "arr = [300, 60, 10]\n",
    "n = int(input())\n",
    "\n",
    "answer= []\n",
    "\n",
    "for i in arr:\n",
    "    answer.append(n // i)\n",
    "    n %= i\n",
    "\n",
    "if n > 0:\n",
    "    print(-1)\n",
    "else:\n",
    "    for i in answer:\n",
    "        print(i, end=\" \")"
   ]
