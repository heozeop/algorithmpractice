    "n = input()\n",
    "n = sorted(n, reverse=True)\n",
    "sum = 0\n",
    "if '0' not in n:\n",
    "    print(-1)\n",
    "else:\n",
    "    for i in n:\n",
    "        sum += int(i)\n",
    "        \n",
    "    if sum % 3 == 0:\n",
    "        print(''.join(n))\n",
    "    else:\n",
    "        print(-1)"
