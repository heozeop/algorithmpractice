    "arr = []\n",
    "a = 0\n",
    "b = 0\n",
    "maxVal = 0\n",
    "\n",
    "for i in range(9):\n",
    "    arr.append(list(map(int, input().split())))\n",
    "\n",
    "for i in range(9): \n",
    "    for j in range(9):\n",
    "        if maxVal < arr[i][j]:\n",
    "            maxVal = arr[i][j]\n",
    "            a = i\n",
    "            b = j\n",
    "\n",
    "print(maxVal)\n",
    "print(f\"{a + 1} {b + 1}\")"
